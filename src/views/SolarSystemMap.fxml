<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane id="AnchorPane" fx:id="anchor" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="returnButton" layoutX="15.0" layoutY="355.0" mnemonicParsing="false"
                onMouseClicked="#returnToUniverse" prefHeight="30.0" text="Return to Universe"/>
        <Label fx:id="locationLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="14.0"
               text="SolarSystemName" textAlignment="CENTER"/>
    </children>
</AnchorPane>
